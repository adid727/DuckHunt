<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.project.AdidNissan.finalproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Duck hunt"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:screenOrientation="landscape" android:configChanges="orientation|keyboardHidden"  android:name="com.project.AdidNissan.finalproject.Main">
        </activity>
        <activity android:screenOrientation="landscape" android:configChanges="orientation|keyboardHidden"  android:name="com.project.AdidNissan.finalproject.SplashActivity">
            <intent-filter>

                <action android:name="android.intent.action.MAIN"
                        android:theme ="@android:style/Theme.Black.NoTitleBar.Fullscreen"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:screenOrientation="landscape" android:configChanges="orientation|keyboardHidden" android:name="com.project.AdidNissan.finalproject.result">
        </activity>
        <activity android:screenOrientation="landscape" android:configChanges="orientation|keyboardHidden"  android:name="com.project.AdidNissan.finalproject.Menu">
        </activity>

    </application>

</manifest>